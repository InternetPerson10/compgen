{
    "python3": {
        "compile": "{python3} -m py_compile {filename}",
        "run": "{python3} {filename}",
        "endings": [".py", ".py3"]
    },
    "python2": {
        "compile": "python2 -m py_compile {filename}",
        "run": "python2 {filename}",
        "endings": [".py2"]
    },
    "java": {
        "compile": "javac {filename}",
        "run": "java -Xmx512m -Xss256m -cp {filename_path} {filename_base}",
        "endings": [".java"]
    },
    "cpp": {
        "compile": "g++ -O2 -std=c++17 {filename} -o {filename_base}.exe",
        "run": ".{sep}{filename_base}.exe",
        "endings": [".c", ".cpp", ".c++"]
    }
}
