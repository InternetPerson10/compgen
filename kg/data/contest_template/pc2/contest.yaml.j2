# Contest Configuration, generated from KompGen
# Created: {{ datetime_created }}
--- 

name: '{{ contest.title }}'
short-name: 'null'
elapsed: 0:00:00
remaining: {{ contest.duration | hms }}
running: false
auto-stop-clock-at-end: false
start-time: 2111-11-11 11:11:11 PHT
duration: {{ contest.duration | hms }}
scoreboard-freeze-length: {{ contest.scoreboard_freeze_length | hms }}
# load-data-files: false
judge-config-path: {{ alldata }}

default-clars:
  - No response, read problem statement

clar-categories:
  - General

languages:
{% for lang in langs %}  - name: '{{ lang.display_name }}'
    active: true
    compilerCmd: '{{ lang.compile }}'
    exemask: '{{ lang.compiled_file }}'
    execCmd: '{{ lang.run }}'
    runner: '{{ lang.run_first }}'
    runner-args: '{{ lang.run_rest }}'
    interpreted: {{ lang.interpreted }}
    use-judge-cmd: false
    judge-exec-cmd: '{{ lang.run }}'

{% endfor %}

sites:
  - number: 1
    name: 'Site 1'
    password: '{{ contest.site_password }}'
    IP: localhost
    port: 50002


accounts:
  - account: TEAM
    site: 1
    count: {{ contest.teams | length }}

  - account: JUDGE
    site: 1
    count: {{ contest.judges | length }}

  - account: SCOREBOARD
    site: 1
    count: {{ contest.leaderboards | length }}

  - account: FEEDER
    site: 1
    count: {{ contest.feeders | length }}

  - account: ADMINISTRATOR
    site: 1
    count: {{ contest.admins | length }}


# EOF Contest Configuration

